{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
    
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "import/newline-after-import": "off"
  }
}